#!/bin/bash
mappings=(
18/18/18-Black
ab/46/42-Red
a1/b5/6c-Green
f7/ca/88-Yellow
7c/af/c2-Blue
ba/8b/af-Magenta
86/c1/b9-Cyan
d8/d8/d8-White
58/58/58-Bright_Black
ab/46/42-Bright_Red
a1/b5/6c-Bright_Green
f7/ca/88-Bright_Yellow
7c/af/c2-Bright_Blue
ba/8b/af-Bright_Magenta
86/c1/b9-Bright_Cyan
f8/f8/f8-Bright_White
dc/96/56
a1/69/46
28/28/28
38/38/38
b8/b8/b8
e8/e8/e8
)
colors=(
  base00
  base08
  base0B
  base0A
  base0D
  base0E
  base0C
  base05
  base03
  base08
  base0B
  base0A
  base0D
  base0E
  base0C
  base07
  base09
  base0F
  base01
  base02
  base04
  base06
)
for i in `seq 0 21`; do
  printf "\x1b[38;5;%smcolor%02s %s %-30s \x1b[48;5;%sm                                   \x1b[0m\n" $i $i ${colors[i]} ${mappings[i]} $i
done;
